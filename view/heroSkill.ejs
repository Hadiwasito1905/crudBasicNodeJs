<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>HERO SKILL</title>
</head>
<body>
<!-- Hero Skill Table -->

<div class="container">
    <h1><%= skill.title %></h1>
    <br />
    <table class="table table-striped table-hover">
        <thead>

        <tr>
            <th scope="col">No.</th>
            <th scope="col">nama</th>
            <th scope="col">SKILL 1</th>
            <th scope="col">SKILL 2</th>
            <th scope="col">SKILL 3</th>
            <th scope="col">SKILL PASIF</th>
        </tr>
        </thead>
        <tbody>

        <% for(let i = 0; i < skill.data.length; i++) { %>
            <tr>
                <!-- <td><%= heroes.data[i].id %></td> -->
                <td><%= i+1 %></td>
                <td><%= skill.data[i].name %></td>
                <td><%= skill.data[i].skill_1 %></td>
                <td><%= skill.data[i].skill_2 %></td>
                <td><%= skill.data[i].skill_3 %></td>
                <td><%= skill.data[i].pasif_skill %></td>

            </tr>
        <% } %>
        </tbody>
    </table>
</div>
</body>
</html>